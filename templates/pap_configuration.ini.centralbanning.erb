[paps]
centralbanning.type = remote
centralbanning.enabled = <%= scope['argus::centralbanning_hostname'] ? "true" : "false" %>
centralbanning.dn = <%= scope['argus::centralbanning_dn']%>
centralbanning.hostname = <%= scope['argus::centralbanning_hostname']%>
centralbanning.port = <%= scope['argus::centralbanning_port']%>
centralbanning.path = /pap/services/
centralbanning.protocol = https
centralbanning.public = <%= scope['argus::centralbanning_public'] ? "true" : "false" %>

[paps:properties]
poll_interval = 14400
ordering = centralbanning
ordering = default

